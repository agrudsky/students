<body>
  <div class="container">
    <div class="row">
      <div class="col-md-8 col-md-offset-2">
        <h1><%= @student.name %>  - Student #<%= @student.id %>  </h1>
        <div>
          Enrolled in: <br>
          <ul>
            <% @student.enrollments.each do |enrollment| %>
            <li><%= enrollment.course.name %></li>
             </ul>
      <td><a href="/courses/<%= enrollment.course.id %>/">Show</a></td> |
      <td><a href="/courses/<%= enrollment.course.id %>/edit">Edit</a></td> |
      <td><a href="/courses/<%= enrollment.course.id %>/destroy">Destroy</a></td>
             <% end %>
       </div>
    </div>
  </div>
  </div>
</body>
  

  
  